curl localhost:8083/history \
-X POST \
--header "Content-Type: application/json" \
--data '{"device_id": "test"}'


curl localhost:8083/history \
-X POST \
--header "Content-Type: application/json" \
--data '{"device_id": "test", "data": "some string for test"}'


curl localhost:8083/history \
-X GET \
--header "Content-Type: application/json" \
--data '{"device_id": "test"}'